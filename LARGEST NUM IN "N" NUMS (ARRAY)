READ "N" ELEMENTS AND FIND THE LARGEST NUM--(ARRAYS):

#include <stdio.h>

int main() {
    // Write C code here
    int n;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int max=a[0];
    for(i=0;i<n;i++){
        if(max<a[i]){
            max=a[i];
        }
    }
printf("%d",max);
    return 0;
}
